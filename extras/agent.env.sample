# 拷贝到：~/.config/woodpecker/agent.env，并配合 setenv.sh + plist 开机自动注入
WOODPECKER_AGENT_NAME=cloud-mac-mini-01
WOODPECKER_SERVER=ci-agent.jmsu.top:443
WOODPECKER_AGENT_SECRET=CHANGE_ME

# 建议项（走 Traefik TLS）
WOODPECKER_GRPC_SECURE=true
WOODPECKER_GRPC_VERIFY=true

# 可选项
WOODPECKER_MAX_WORKFLOWS=1
WOODPECKER_HEALTHCHECK_ADDR=:3001
WOODPECKER_AGENT_LABELS="platform=darwin/arm64,gpu=metal,host=$(hostname)"
